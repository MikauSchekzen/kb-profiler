{
  "cmd": "mingw32-make.exe",
  "name": "Build",
  "cwd": "{PROJECT_PATH}",
  "sh": false,
  "targets": {
    "alt": {
      "name": "Build Detector",
      "cmd": "mingw32-g++.exe",
      "cwd": "{PROJECT_PATH}",
      "args": ["-L.\\x86", "-L.\\src", "-o", "HWID_Detector.exe", "detector.cpp", "src\\utils.c", "x86\\interception.lib"],
      "sh": false
    },
    "alt2": {
      "name": "Meow",
      "cmd": "mingw32-g++.exe",
      "cwd": "{PROJECT_PATH}",
      "args": ["profiler.cpp", "-o", "D:\\Meow.exe"],
      "sh": false
    }
  }
}
